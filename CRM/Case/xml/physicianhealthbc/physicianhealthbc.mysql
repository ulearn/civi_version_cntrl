-- Update number of allowed locations
UPDATE civicrm_preferences set location_count='2';

-- Set settings for outbound email
UPDATE civicrm_preferences set mailing_backend='a:9:{s:5:\"qfKey\";s:32:\"fd260b00747b1f2331935d7977c50727\";s:15:\"outBound_option\";s:1:\"0\";s:13:\"sendmail_path\";s:17:\"/usr/bin/sendmail\";s:13:\"sendmail_args\";s:2:\"-i\";s:10:\"smtpServer\";s:9:\"localhost\";s:8:\"smtpPort\";s:2:\"25\";s:8:\"smtpAuth\";s:1:\"0\";s:12:\"smtpUsername\";s:0:\"\";s:12:\"smtpPassword\";s:0:\"\";}';

-- Set FROM email address
SELECT @fromEmailId := max(id) from civicrm_option_group where name = 'from_email_address';
UPDATE civicrm_option_value SET label='\"Physician Health Program\" <info@physicianhealth.com>' WHERE option_group_id=@fromEmailId AND is_default=1;

-- Put Dr at the top of the prefix list
SELECT @prefixId := max(id) from civicrm_option_group where name = 'individual_prefix';
UPDATE civicrm_option_value SET weight=1 WHERE option_group_id=@prefixId AND name='Dr';
UPDATE civicrm_option_value SET weight=2 WHERE option_group_id=@prefixId AND name='Mr';
UPDATE civicrm_option_value SET weight=3 WHERE option_group_id=@prefixId AND name='Mrs';
UPDATE civicrm_option_value SET weight=4 WHERE option_group_id=@prefixId AND name='Ms';

-- Add a core activity called Correspondence which we use mostly to record contracts but also as part of a general filing system.
SELECT @actId := max(id) from civicrm_option_group where name = 'activity_type';
SELECT @maxval := max(value) from civicrm_option_value where option_group_id=@actId;
SELECT @maxweight := max(weight) from civicrm_option_value where option_group_id=@actId;
